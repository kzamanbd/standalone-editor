{
    "name": "standalone-compiler",
    "version": "1.0.0",
    "description": "This is a full stack application with react and node",
    "engines": {
        "node": ">=18"
    },
    "packageManager": "pnpm@10.12.4",
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "format": "prettier --write \"**/*.{ts,tsx}\""
    },
    "keywords": [
        "react",
        "node",
        "express",
        "socket.io"
    ],
    "author": "Kamruzzaman",
    "license": "MIT",
    "devDependencies": {
        "@repo/eslint-config": "workspace:*",
        "@repo/typescript-config": "workspace:*",
        "prettier": "^3.6.2",
        "turbo": "^2.5.4",
        "typescript": "^5.8.3"
    }
}
